{
  "name": "toki-backend",
  "version": "1.0.19",
  "description": "Backend API for Toki social map app",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build:admin && nodemon src/index.ts",
    "build": "npm run build:admin && tsc",
    "build:admin": "cd admin-panel && npm install && npm run build",
    "prestart": "npm run build:admin",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "ts-node src/scripts/run-migrations.ts",
    "seed:notifications": "ts-node src/scripts/seed-notifications.ts"
  },
  "keywords": [
    "toki",
    "social",
    "map",
    "api",
    "express",
    "typescript"
  ],
  "author": "Toki Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@supabase/supabase-js": "^2.52.1",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo-server-sdk": "^4.0.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1"
